{
  "name": "nodejs-design-patterns-book",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build",
    "test": "eslint src",
    "build:js": "NODE_ENV=production webpack",
    "build:sass": "NODE_ENV=production sass --style=compressed src/scss/style.scss src/_includes/css/style.css",
    "build:eleventy": "NODE_ENV=production eleventy",
    "watch:js": "webpack --watch",
    "watch:sass": "npm run build:sass -- --watch",
    "watch:eleventy": "eleventy --serve",
    "dev": "npm-run-all build:sass build:js --parallel watch:*",
    "build": "npm-run-all clean build:sass build:js build:eleventy",
    "deploy": "gh-pages -d build --dotfiles",
    "start": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodejs-design-patterns-book/nodejs-design-patterns-book.git"
  },
  "author": "Luciano Mammino, Mario Casciaro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nodejs-design-patterns-book/nodejs-design-patterns-book/issues"
  },
  "homepage": "https://github.com/nodejs-design-patterns-book/nodejs-design-patterns-book#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "@11ty/eleventy-img": "^0.3.0",
    "@babel/core": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "bulma": "^0.9.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.5.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "html-minifier": "^4.0.0",
    "html-webpack-plugin": "^4.3.0",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.10",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "dependencies": {}
}
